public abstract class Vehicule { //abstract parce qu'on fait pas de new Vehicule mais des new Voiture ou new Bus
	public int nb_personnes;
	public int nb_empl;
	
	public Vehicule (int p, int e) {
		this.nb_personnes = p;
		this.nb_empl = e;
	}
	
	public int tarif() {
		int prix = 75 * nb_empl + 15* nb_personnes;
		return prix;
	}
	
	public static void main(String[] args) {
	}

}


